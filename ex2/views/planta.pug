extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Planta
    .w3-container
      table.w3-table-all
        tr
          td ID 
          td= p._id
        tr 
          td Número de Registo
          td= p.NumeroDeRegisto
        tr 
          td Código de Rua
          td= p.CodigoDeRua
        tr
          td Rua 
          td= p.Rua
        tr 
          td Local
          td= p.Local
        tr 
          td Freguesia
          td= p.Freguesia
        tr
          td Espécie 
          td= p.Especie
        tr 
          td Nome Científico
          td= p.NomeCientifico
        tr 
          td Origem
          td= p.Origem
        tr
          td Data de Plantação 
          td= p.DataDePlantacao
        tr 
          td Estado
          td= p.Estado
        tr 
          td Caldeira
          td= p.Caldeira
        tr
          td Tutor 
          td= p.Tutor
        tr 
          td Implantação
          td= p.Implantacao
        tr 
          td Gestor
          td= p.Gestor
        tr
          td Data de Atualização 
          td= p.DataDeActualizacao
        tr 
          td Número de Intervenções
          td= p.NumeroDeIntervencoes
              
    footer.w3-container.w3-teal
      h5 Generated by Exame::EngWeb2023 in #{d}
      a(href="/") [Página Principal]